<h1>My Port polio</h1>
<div>
    <%= form_tag 'search_stock',remote: true, method: :get do %>
        <div class="form-group row">
            <div class="col-sm-9">
                <%= text_field_tag :stock, params[:stock], placeholder: "search", autofocus: true, class: "form-control form-control-lg" %>
            </div>
            <div class="col-sm-3">
                <%= button_tag type: :submit, class: "btn btn-success" do %>
                <%= "Search" %>
                <% end %>
            </div>
        </div>
    <% end %>
</div>
<div  id="results">

</div>
<%= render 'users/result' %>